{
  "name": "megastore",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --mode=developmentDebug",
    "android:dev": "react-native run-android --mode=developmentdebug",
    "android:dev-release": "react-native run-android --mode=developmentrelease",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme 'megastoreDev' --simulator 'iPhone 16 Pro Max'",
    "ios:prod": "react-native run-ios --scheme 'megastoreProd --simulator 'iPhone 14'",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "resetcache": "yarn start -- --reset-cache",
    "build:dev-apk": "cd android && ./gradlew assembleDevelopmentRelease && cd ..",
    "build:prod-apk": "cd android && ./gradlew assembleProductionRelease && cd ..",
    "build:dev-bundle": "cd android && ./gradlew bundleDevelopmentRelease && cd ..",
    "build:prod-bundle": "cd android && ./gradlew bundleProductionRelease && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "pod-update": "cd ios && pod install --repo-update && cd ..",
    "clean:gradle": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@react-navigation/drawer": "^7.1.2",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/react-native-indicators": "^0.16.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-plugin-module-resolver": "^5.0.2",
    "formik": "^2.4.6",
    "immutable": "^5.0.3",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-config": "^1.5.5",
    "react-native-flash-message": "^0.4.2",
    "react-native-formik": "^1.7.8",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-indicators": "^0.17.0",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.1",
    "react-native-svg": "^15.11.2",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-version-info": "^1.1.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "rn-wave-bottom-bar": "^2.2.36",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
